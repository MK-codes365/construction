{
  "version": 2,
  "builds": [
    {
      "src": "src/app/**/*",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_ALCHEMY_API_KEY": "@alchemy_api_key",
    "NEXT_PUBLIC_ETH_RPC_URL": "@eth_rpc_url"
  },
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"]
}